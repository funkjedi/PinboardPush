{
  "name": "Pinboard Push",
  "version": "0.1.3",
  "description": "An extension that pushes new bookmarks and bookmark changes to Pinboard.",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "https://api.pinboard.in/v1/*",
    "bookmarks",
    "storage"
  ],
  "icons": {
    "16": "img/16x16.png",
    "48": "img/48x48.png",
    "128": "img/128x128.png"
  },
  "manifest_version": 3
}
